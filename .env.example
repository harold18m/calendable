# Google OAuth
# Obt√©n estos valores en https://console.cloud.google.com/
GOOGLE_CLIENT_ID=tu_client_id
GOOGLE_CLIENT_SECRET=tu_client_secret

# NextAuth
# Genera un secret con: openssl rand -base64 32
NEXTAUTH_SECRET=tu_secret_key_generado
NEXTAUTH_URL=http://localhost:3000

# AWS Bedrock
# Configura un usuario IAM con permisos de Bedrock
AWS_ACCESS_KEY_ID=tu_access_key
AWS_SECRET_ACCESS_KEY=tu_secret_key
AWS_REGION=us-east-1

# Modelo de IA (opcional)
# Opciones: claude-3-haiku, claude-3.5-sonnet, claude-sonnet-4
BEDROCK_MODEL=claude-3-haiku
